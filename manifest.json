{
    "manifest_version": 3,
    "name": "AutoAct",
    "version": "1.0.0",
    "description": "",
    "content_scripts": [
        {
            "js": ["src/content.tsx"],
            "matches": ["<all_urls>"]
        }
    ],
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": ["sidePanel"],
    "background": {
        "service_worker": "src/background.js"
    }
}
